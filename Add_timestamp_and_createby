CREATE OR REPLACE STREAM transaction_test AS
SELECT 
  CUSTOMER_ID, 
  MONTH, 
  CATEGORY, 
  PAYMENT_TYPE, 
  SPEND,
  TRANSACTION_ID,
  TIMESTAMPTOSTRING(ROWTIME, 'yyyy-MM-dd HH:mm:ss') AS CREATE_DATE,
  'vamshi' AS CREATED_BY,
  TIMESTAMPTOSTRING(ROWTIME, 'yyyy-MM-dd HH:mm:ss') AS UPDATE_DATE,
  'vamshi' AS UPDATED_BY
FROM transaction_stream;
